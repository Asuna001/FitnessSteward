
.title {
  text-align: center;
  font-size: 45rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-bottom: 20rpx;
}
.container {
  background-color: #6fc599;
  padding-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: auto;
  justify-content: flex-start;
}



.form {
  background-color: #F2FFF2;
}

.upload-box{
  margin-top: 50rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 80%;
}
.upload-btn{
  border-radius: 10rpx;
  background-color: white;
  width: 400 rpx;
  font-size: larger;
  margin-bottom: 40rpx;
  padding: 20rpx;
  text-align: center;
}

.video{
  width:80%;
  height: 400rpx;
  padding: 10rpx;
  background-color: white;
}
